Tab2:AddButton({
	Name = "Artifacts/Auras",
	Callback = function()
	local gui = game.Players.LocalPlayer.PlayerGui:WaitForChild("Frames"):WaitForChild("Artifacts")
		if gui.Enabled == false then
       	 gui.Enabled = true
			else
				gui.Enabled = false
		end
  	end 
})


